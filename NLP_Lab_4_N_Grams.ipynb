      ],
      "source": [
        "def generate_ngrams(text, n):\n",
        "  tokens = nltk.word_tokenize(text)\n",
        "  ngrams = list(nltk.ngrams(tokens, n))\n",
        "  return ngrams\n",
        "\n",
        "\n",
        "i = \"I like my Cake Pink\"\n",
        "n = 3\n",
        "\n",
        "ngrams = generate_ngrams(i, n)\n",
        "print(ngrams)"
      ]
